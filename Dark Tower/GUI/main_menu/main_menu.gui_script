function init(self)
	msg.post(".", "acquire_input_focus")		
end

function on_input(self, action_id, action)
	if action_id == hash("click") and action.pressed then				
		if gui.pick_node(gui.get_node("btn_newgame"), action.x, action.y) then
			msg.post("main_col:/manager#manager", "show_game")
		end				
	end
end	